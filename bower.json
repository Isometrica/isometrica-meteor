{
  "name": "isometrica-meteor",
  "version": "0.0.0",
  "homepage": "https://github.com/Isometrica/isometrica-meteor",
  "authors": [
    "Mark Leusink <m.leusink@gmail.com>"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "public/vendor/",
    "test",
    "tests"
  ],
  "dependencies": {
    "sub-stance": "https://github.com/Isometrica/sub-stance.git",
    "textAngular": "~1.4.1",
    "ng-tags-input": "~2.3.0",
    "bootcards": "~1.1.2",
    "angular-growl-v2": "~0.7.5",
    "angular-formly-templates-bootstrap": "~6.0.0",
    "angular-filter": "~0.5.5"
  },
  "ignoredDependencies": [
    "angular",
    "bootstrap"
  ],
  "resolutions": {
    "angular": ">=1.2.1"
  },
  "private": true,
  "overrides": {
    "textAngular": {
      "arch": [
        "client"
      ]
    },
    "ng-tags-input": {
      "arch": [
        "client"
      ],
      "main": [
        "ng-tags-input.min.js",
        "ng-tags-input.min.css",
        "ng-tags-input.bootstrap.min.css"
      ]
    },
    "bootcards": {
      "arch": [
        "client"
      ],
      "main": [
        "dist/css/bootcards-desktop.min.css"
      ]
    },
    "angular-growl-v2": {
      "arch": [
        "client"
      ],
      "main": [
        "build/angular-growl.min.css",
        "build/angular-growl.min.js"
      ]
    }
  }
}
